@prefix app: <http://application-inventory/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://application-inventory/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<http://application-inventory> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://application-inventory#exposedTo
app:exposedTo rdf:type owl:ObjectProperty ;
           owl:inverseOf app:exposedIn ;
           rdfs:domain app:Package ;
           rdfs:range app:Vulnerability .


###  http://application-inventory/deployedTo
app:deployedTo rdf:type owl:ObjectProperty ;
            rdfs:domain app:Application ;
            rdfs:range app:Host .

###  http://application-inventory/exposedIn
app:exposedIn rdf:type owl:ObjectProperty ;
              rdfs:domain app:Vulnerability ;
              rdfs:range app:Package .


###  http://application-inventory/includedIn
app:includedIn rdf:type owl:ObjectProperty ;
               owl:inverseOf app:includes ;
               rdfs:domain app:Package ;
               rdfs:range app:Application .


###  http://application-inventory/includes
app:includes rdf:type owl:ObjectProperty ;
             rdfs:domain app:Application ;
             rdfs:range app:Package .


#################################################################
#    Data properties
#################################################################

###  http://application-inventory/deployedOn
app:deployedOn rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;            
            rdfs:domain << app:Application app:deployedTo app:Host >> ;                     
            rdfs:range xsd:dateTime .


###  http://application-inventory/description
app:description rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain app:Thing ; 
             rdfs:range xsd:string .

###  http://application-inventory/version
app:version rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
          rdfs:domain app:Package ;
          rdfs:range xsd:string .

###  http://application-inventory/name
app:name rdf:type owl:DatatypeProperty ,
               owl:FunctionalProperty ;
      rdfs:domain app:Thing ;                                    
      rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################


###  http://application-inventory/Vulnerable
app:Vulnerable owl:equivalentClass [
    owl:unionOf  ( 
                  [rdf:type owl:Restriction ;
                  owl:onProperty app:exposedTo ;
                  owl:someValuesFrom app:Vulnerability]

                  [rdf:type owl:Restriction ;
                  owl:onProperty app:includes ;
                  owl:someValuesFrom app:Vulnerable]
    ) 
];
rdf:type owl:Class;
rdfs:subClassOf app:Thing .


###  http://application-inventory/Application
app:Application rdf:type owl:Class;
             rdfs:subClassOf app:Thing .


###  http://application-inventory/Host
app:Host rdf:type owl:Class;
      rdfs:subClassOf app:Thing .


###  http://application-inventory/Package
app:Package rdf:type owl:Class;
         rdfs:subClassOf app:Thing .


###  http://application-inventory/Vulnerability
app:Vulnerability rdf:type owl:Class ;
               rdfs:subClassOf prov:Entity ;
               prov:wasGeneratedBy  app:VulnerabilityReportingActivity .

app:VulnerabilityReportingActivity rdf:type owl:Class ;
                                rdfs:subClassOf prov:Activity; 
                                prov:wasAssociatedWith app:SecurityAgent;
                                prov:used  app:VulnerabilityScanningResults .

app:SecurityAgent rdf:type owl:Class ;                            
               rdfs:subClassOf prov:Agent .      

app:VulnerabilityScanningResults rdf:type owl:Class ;                            
                              rdfs:subClassOf prov:Entity .

app:Thing rdf:type owl:Class .



